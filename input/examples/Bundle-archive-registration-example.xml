        <id value="MessageHeader-0006"/>
        <!--消息事件 1..1-->
        <eventCoding>
          <!-- 0..1 事件类型 CodeSystem url  -->
          <system value="http://fhir.scwjxx.cn/Sichuan/CodeSystem/bussiness-event-code"/>
          <!-- 0..1 事件类型代码-->
          <code value="C02002"/>
          <!-- 0..1 事件类型名称 -->
          <display value="家庭患者档案注册"/>
        </eventCoding>
        <!-- 1..1 消息源应用程序 -->
        <source>
          <!-- 0..1 系统名称（发送消息的系统） -->
          <name value="[string]"/>
          <!-- 0..1 软件名称（子系统名称） -->
          <software value="[string]"/>
          <!-- 0..1 运行的软件版本（当前版本） -->
          <version value="[string]"/>
          <!-- 1..1 ID地址(当前操作客户端IP地址) -->
          <endpoint value="[uri]"/>
        </source>
        <!--消息实际内容 0..*-->
        <focus>
          <!-- 0..1 Reference(Task) 关联工作流信息 -->
          <reference value="Task/ArchiveRegistration-example"/>
        </focus>
        <!-- 0..1 canonical(MessageDefinition) Link to the definition for this message -->
        <definition  value="MessageDefinition/md-archive-registration"/>
      </MessageHeader>
    </resource>
  </entry>

  <!--************************医生团队注册******************************-->
  <entry>
    <resource>
      <Task xmlns="http://hl7.org/fhir">
        <id value="ArchiveRegistration-example"/>
        <!-- 0..1 canonical(ActivityDefinition) 关联ActivityDefinition -->
        <instantiatesCanonical value="http://fhir.scwjxx.cn/Sichuan/ActivityDefinition/ad-team-registration"/>
        <!-- 1..1 状态 draft | requested | received | accepted | + -->
        <status value="requested"/>

        <!-- 1..1 目的 unknown | proposal | plan | order | original-order | reflex-order | filler-order | instance-order | option -->
        <intent value="proposal"/>

        <!-- 0..1 CodeableConcept 事件类型 -->
        <businessStatus>
          <coding>
            <!-- 0..1 事件类型 CodeSystem url  -->
            <system value="http://fhir.scwjxx.cn/Sichuan/CodeSystem/bussiness-event-code"/>
            <!-- 0..1 事件类型代码-->
            <code value="C02002"/>
            <!-- 0..1 事件类型名称 -->
            <display value="家庭患者档案注册"/>
          </coding>
        </businessStatus>

        <!-- 0..1 CodeableConcept Task Type 任务类型-->
        <code>
          <coding>
            <!-- 0..1 任务类型 CodeSystem url  -->
            <system value="http://hl7.org/fhir/CodeSystem/task-code"/>
            <!-- 0..1 任务类型代码-->
            <code value="approve"/>
            <!-- 0..1 任务类型名称 -->
            <display value="Activate/approve the focal resource"/>
          </coding>
        </code>
        <!-- 0..1 任务描述 -->
        <description value="多名医生通过注册组成可签约的家庭医生团队，并且注册该团队的信息。"/>
        <!-- 0..1 Reference(Any) 关联该任务正在处理的资源 -->
        <focus>
          <reference value="Group/Family-example"/>
        </focus>
        <!-- ?? 0..1 任务创建日期 -->
        <authoredOn value="2019-02-10T13:00:00Z"/>
        <!-- ?? 0..1 最后更新日期 -->
        <lastModified value="2019-02-10T13:00:00Z"/>
        <!-- 0..1 任务创建者，关联医护人员 -->
        <requester>
          <reference value="Practitioner/Practitioner001-example"/>
        </requester>

        <!-- 0..* 任务备注 -->
        <note>
          <!-- 1..1 The annotation  - text content (as markdown) -->
          <text value="[markdown]"/>
        </note>
      </Task>
    </resource>
  </entry>
