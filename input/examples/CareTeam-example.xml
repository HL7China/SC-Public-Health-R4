<?xml version="1.0" encoding="utf-8" ?> 
  <CareTeam xmlns="http://hl7.org/fhir">
        <id value="CareTeam-example"/>
        <meta>
          <profile value="http://StructureDefinition.hl7fhir.cn/family-doctor-contract-team"/>
        </meta>


        <!-- 0..1 团队注册号 -->
        <identifier>
          <!--0..1 团队注册号NamingSystem uri-->
          <system value="http://NamingSystem.hl7fhir.cn/team-registration-identifier"/>
          <!-- 0..1 团队注册号 -->
          <value value="[string]"/>
        </identifier>

        <!-- 0..1 团队状态  proposed | active | suspended | inactive | entered-in-error -->
        <status value="active"/>

        <!-- 0..* 团队类型 -->
        <category>
          <coding>
            <!-- 团队类型  CodeSystem  地址 -->
            <system value="http://CodeSystem.hl7fhir.cn/CareTeam-type"/>
            <!-- 0..1 团队类型-->
            <code value="01"/>
            <!-- 0..1 团队类型名称 -->
            <display value="家庭医生团队"/>
          </coding>
        </category>
        <!-- 0..1 团队名称 -->
        <name value="家庭医生团队1"/>

        <!-- 0..1 Reference(Group) 服务范围 -->
        <subject>
          <!-- 0..1 Reference(Group) 关联范围组信息 -->
          <reference value="Group/ServiceArea-example"/>
        </subject>

        <!-- 0..1 团队存续时间 -->
        <period>
          <!--  0..1 团队创建时间 -->
          <start value="2020-06-06T09:12:25.612+00:00"/>
          
        </period>

        <!--  0..* 团队成员 -->
        <participant>
          <!-- 0..* 成员角色 -->
          <role>
            <coding>
              <!-- 成员角色  CodeSystem  地址 -->
              <system value="http://CodeSystem.hl7fhir.cn/participant-role"/>
              <!-- 0..1 成员角色-->
              <code value="01"/>
              <!-- 0..1 成员角色名称 -->
              <display value="医生"/>
            </coding>
            <!-- 0..1 成员角色和该角色所履行职责的文字描述 -->
            <text value="张三"/>
          </role>
          <!-- 0..1 Reference(Practitioner）成员 -->
          <member>
            <!-- 0..1 Reference(Practitioner) 关联签约医生信息 -->
            <reference value="Practitioner/Practitioner001-example"/>
          </member>
          <!-- 0..1 Reference(Organization) 成员所属组织 -->
          <onBehalfOf>
            <!-- 0..1 Reference(Organization) 关联成员所属组织信息 -->
            <reference value="Organization/Hospital001-example"/>
          </onBehalfOf>
          <!-- 0..1 成员再团时间 -->
          <period>
            <!--  0..1 入团时间 -->
            <start value="2020-06-06T09:12:25.612+00:00"/>
          
          </period>
        </participant>

        <!--  0..* 团队成员 -->
        <participant>
          <!-- 0..* 成员角色 -->
          <role>
            <coding>
              <!-- 成员角色  CodeSystem  地址 -->
              <system value="http://CodeSystem.hl7fhir.cn/participant-role"/>
              <!-- 0..1 成员角色-->
              <code value="01"/>
              <!-- 0..1 成员角色名称 -->
              <display value="医生"/>
            </coding>
            <!-- 0..1 成员角色和该角色所履行职责的文字描述 -->
            <text value="李四"/>
          </role>
          <!-- 0..1 Reference(Practitioner）成员 -->
          <member>
            <!-- 0..1 Reference(Practitioner) 关联签约医生信息 -->
            <reference value="Practitioner/Practitioner002-example"/>
          </member>
          <!-- 0..1 Reference(Organization) 成员所属组织 -->
          <onBehalfOf>
            <!-- 0..1 Reference(Organization) 关联成员所属组织信息 -->
            <reference value="Organization/Hospital001-example"/>
          </onBehalfOf>
          <!-- 0..1 成员再团时间 -->
          <period>
            <!--  0..1 入团时间 -->
            <start value="2020-06-06T09:12:25.612+00:00"/>

          </period>
        </participant>



        <!-- 0..* Reference(Organization) 创建机构 -->
        <managingOrganization>
          <!-- 0..1 Reference(Organization) 关联机构信息 -->
          <reference value="Organization/Hospital001-example"/>
        </managingOrganization>

        <!-- 0..1 联系电话 -->
        <telecom>
          <!--  0..1 联系方式类型 phone | fax | email | pager | url | sms | other -->
          <system value="phone"/>
          <!-- 0..1 联系电话 -->
          <value value="13308012373"/>
        </telecom>
        <!-- 0..1 联系地址 -->
        <telecom>
          <!--  0..1 联系方式类型 phone | fax | email | pager | url | sms | other -->
          <system value="Other"/>
          <!-- 0..1 联系地址 -->
          <value value="XX市XX县XXXXX路XXX号"/>
        </telecom>

        <!--0..1 团队介绍 -->
        <extension url="http://FHIR.PRESS/StructureDefinition/brief-Introduction">
          <valueString value="团队介绍信息XXXXXXXXXXXXX"/>
        </extension>
      </CareTeam>