              <!-- 0..1 申请意图代码-->
              <code value="plan"/>
              <!-- 0..1 申请意图说明 -->
              <display value="plan"/>
            </valueCoding>
          </extension>
        </intent>
        <!-- 0..1 申请日期 -->
        <authoredOn value="2019-02-10T13:00:00Z"/>

        <!-- 0..* Reference(CareTeam) 执行团队 -->
        <performer>
          <!-- 0..1 Reference(CareTeam) 关联签约团队信息 -->
          <reference value="CareTeam/TeamRegistration-example"/>
        </performer>
      </ServiceRequest>
    </resource>
  </entry>

